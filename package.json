{
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:codegen": "graphql-codegen --config codegen.yml --watch",
    "dev:path": "pathpida --watch",
    "dev:storybook": "start-storybook -s ./public -p 6006",
    "tsc": "tsc",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "format": "prettier . --write",
    "fix": "yarn lint --fix && yarn format --loglevel error",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "*.{css,scss,json,js,jsx,ts,tsx,yaml}": "prettier --write"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.0.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "lint-staged": "^12.3.7",
    "storybook": "^6.4.19",
    "typescript": "4.0"
  }
}
